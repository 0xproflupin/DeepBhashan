{
 "fragments": [
  {
   "begin": "0",
   "children": [],
   "end": "4.16",
   "id": "f000001",
   "language": "en",
   "lines": [
    "dev ko teen din baad phir dard hua"
   ]
  },
  {
   "begin": "4.16",
   "children": [],
   "end": "8.56",
   "id": "f000002",
   "language": "en",
   "lines": [
    "saaree raat veena usakee maalish karatee rahee"
   ]
  },
  {
   "begin": "8.56",
   "children": [],
   "end": "13.04",
   "id": "f000003",
   "language": "en",
   "lines": [
    "ekek karake dev ne dard kee chaar goliyaan kha leen"
   ]
  },
  {
   "begin": "13.04",
   "children": [],
   "end": "15.68",
   "id": "f000004",
   "language": "en",
   "lines": [
    "tab kaheen vah so paaya"
   ]
  },
  {
   "begin": "15.68",
   "children": [],
   "end": "21.76",
   "id": "f000005",
   "language": "en",
   "lines": [
    "subahasavere veena ne amit se dev kee dasha ke vishay mein baat kee"
   ]
  },
  {
   "begin": "21.76",
   "children": [],
   "end": "25.24",
   "id": "f000006",
   "language": "en",
   "lines": [
    "vah bhee chintit ho utha"
   ]
  },
  {
   "begin": "25.24",
   "children": [],
   "end": "28.6",
   "id": "f000007",
   "language": "en",
   "lines": [
    "dev us samay baatharoom mein tha"
   ]
  },
  {
   "begin": "28.6",
   "children": [],
   "end": "40.1",
   "id": "f000008",
   "language": "en",
   "lines": [
    "veena ke paas samay tha amit se baat karane ka bolee vah  amit us din doktar gupta kee baat sunakar mujhe dar lag gaya"
   ]
  },
  {
   "begin": "40.1",
   "children": [],
   "end": "50.92",
   "id": "f000009",
   "language": "en",
   "lines": [
    "ekaek aisa laga ki jaise vah kahana chaah rahe hon ki aage yadi dard ho to ve doosare kisee aspataal mein jaen"
   ]
  },
  {
   "begin": "50.92",
   "children": [],
   "end": "56.52",
   "id": "f000010",
   "language": "en",
   "lines": [
    "vahaan ke speshalist ko dikhaen aur rediyotharaipee karava len"
   ]
  },
  {
   "begin": "56.52",
   "children": [],
   "end": "63.68",
   "id": "f000011",
   "language": "en",
   "lines": [
    "amit yah sunakar bola  rediyotharaipee aapane us din to mujhe bataaya nahin"
   ]
  },
  {
   "begin": "63.68",
   "children": [],
   "end": "68.88",
   "id": "f000012",
   "language": "en",
   "lines": [
    "use tanik aashchary hua  usakee kya jaroorat hai"
   ]
  },
  {
   "begin": "68.88",
   "children": [],
   "end": "73.24",
   "id": "f000013",
   "language": "en",
   "lines": [
    "vah to bola ki mainne opareshan ke baad hee kaha tha"
   ]
  },
  {
   "begin": "73.24",
   "children": [],
   "end": "78.48",
   "id": "f000014",
   "language": "en",
   "lines": [
    "veena ne sanyat svar mein kaha  ab bhee der nahin huee"
   ]
  },
  {
   "begin": "78.48",
   "children": [],
   "end": "82.64",
   "id": "f000015",
   "language": "en",
   "lines": [
    "dev ko dard baarabaar uth raha hai"
   ]
  },
  {
   "begin": "82.64",
   "children": [],
   "end": "89.28",
   "id": "f000016",
   "language": "en",
   "lines": [
    "aisa kyon kaha usane  apane man ke bhay ko kuchh ujaagar kiya usane"
   ]
  },
  {
   "begin": "89.28",
   "children": [],
   "end": "100.8",
   "id": "f000017",
   "language": "en",
   "lines": [
    "main kya jaanoon bhaabhee usane aisa kyon kaha haan opareshan ke baad usane aisa kuchh nahin kaha tha isaka mujhe poora pata hai"
   ]
  },
  {
   "begin": "100.8",
   "children": [],
   "end": "107.2",
   "id": "f000018",
   "language": "en",
   "lines": [
    "tab vah baat ramesh bhaee ya mohan bhaee se hee karata tha"
   ]
  },
  {
   "begin": "107.2",
   "children": [],
   "end": "110.48",
   "id": "f000019",
   "language": "en",
   "lines": [
    "ve saaree baaten mujhe bata dete the"
   ]
  },
  {
   "begin": "110.48",
   "children": [],
   "end": "115.32",
   "id": "f000020",
   "language": "en",
   "lines": [
    "amit kee baat mein sachchaee thee veena ko laga"
   ]
  },
  {
   "begin": "115.32",
   "children": [],
   "end": "118.92",
   "id": "f000021",
   "language": "en",
   "lines": [
    "veena kee aankhon se aansoo tapak pade"
   ]
  },
  {
   "begin": "118.92",
   "children": [],
   "end": "122.08",
   "id": "f000022",
   "language": "en",
   "lines": [
    "amit bhee bhaavuk ho gaya"
   ]
  },
  {
   "begin": "122.08",
   "children": [],
   "end": "125.56",
   "id": "f000023",
   "language": "en",
   "lines": [
    "bola  bhaabhee duhkhee mat hoeee"
   ]
  },
  {
   "begin": "125.56",
   "children": [],
   "end": "129.72",
   "id": "f000024",
   "language": "en",
   "lines": [
    "main to har kisee se aapakee misaal deta hoon"
   ]
  },
  {
   "begin": "129.72",
   "children": [],
   "end": "135.92",
   "id": "f000025",
   "language": "en",
   "lines": [
    "aapane kitanee himmat kee dev se shaadee kee haan karake"
   ]
  },
  {
   "begin": "135.92",
   "children": [],
   "end": "140.28",
   "id": "f000026",
   "language": "en",
   "lines": [
    "veena ne aansoo ponchhate hue dekha amit kee or"
   ]
  },
  {
   "begin": "140.28",
   "children": [],
   "end": "142.8",
   "id": "f000027",
   "language": "en",
   "lines": [
    "ghar mein sabase chhota"
   ]
  },
  {
   "begin": "142.8",
   "children": [],
   "end": "146.52",
   "id": "f000028",
   "language": "en",
   "lines": [
    "shabdon mein kaheen use chaturaee na dikhee"
   ]
  },
  {
   "begin": "146.52",
   "children": [],
   "end": "152.82",
   "id": "f000029",
   "language": "en",
   "lines": [
    "ekaek kah uthee tum bhee aisa hee samajhate ho ki mujhe pata tha"
   ]
  },
  {
   "begin": "152.82",
   "children": [],
   "end": "160.48",
   "id": "f000030",
   "language": "en",
   "lines": [
    "haan vigyaapan to jo banaaya tha vah dev ne hee likh kar diya tha"
   ]
  },
  {
   "begin": "160.48",
   "children": [],
   "end": "171.68",
   "id": "f000031",
   "language": "en",
   "lines": [
    "shesh main itanee baat jaanata hoon ki dev ne aapase jaroor baat kee hogee ya phir ramesh bhaee ne aapake daidee se"
   ]
  },
  {
   "begin": "171.68",
   "children": [],
   "end": "175.08",
   "id": "f000032",
   "language": "en",
   "lines": [
    "amit ko aashchary ho raha tha"
   ]
  },
  {
   "begin": "175.08",
   "children": [],
   "end": "177.52",
   "id": "f000033",
   "language": "en",
   "lines": [
    "veena ke mukh se aisa sunakar"
   ]
  },
  {
   "begin": "177.52",
   "children": [],
   "end": "183.08",
   "id": "f000034",
   "language": "en",
   "lines": [
    "veena tab bolee  tumhen premasa darja diya hai"
   ]
  },
  {
   "begin": "183.08",
   "children": [],
   "end": "186.64",
   "id": "f000035",
   "language": "en",
   "lines": [
    "apana chhota bhaee maanane lagee hoon"
   ]
  },
  {
   "begin": "186.64",
   "children": [],
   "end": "194.6",
   "id": "f000036",
   "language": "en",
   "lines": [
    "tumase hee kahakar man halka kar rahee hoon ki mujhe is baat ka kuchh pata nahin chala"
   ]
  },
  {
   "begin": "194.6",
   "children": [],
   "end": "204.4",
   "id": "f000037",
   "language": "en",
   "lines": [
    "vigyaapan mein jo likha tha vah to medikal tarm mere kya kisee bhee padhelikhe ko samajh nahin aa sakatee"
   ]
  },
  {
   "begin": "204.4",
   "children": [],
   "end": "210.48",
   "id": "f000038",
   "language": "en",
   "lines": [
    "lekin usake baad kisee ne bhee is baat ka jikr nahin kiya"
   ]
  },
  {
   "begin": "210.48",
   "children": [],
   "end": "216.76",
   "id": "f000039",
   "language": "en",
   "lines": [
    "mere ghar vaale to abhee bhee nahin jaanate ki dev mein koee kamee hai bhee"
   ]
  },
  {
   "begin": "216.76",
   "children": [],
   "end": "225",
   "id": "f000040",
   "language": "en",
   "lines": [
    "sab meree aur dev kee jodee ko dekhadekh kar hee khushee se bhare hain"
   ]
  },
  {
   "begin": "225",
   "children": [],
   "end": "229",
   "id": "f000041",
   "language": "en",
   "lines": [
    "veena kee baat sunakar sann rah gaya amit"
   ]
  },
  {
   "begin": "229",
   "children": [],
   "end": "239.06",
   "id": "f000042",
   "language": "en",
   "lines": [
    "bola  ye kaise ho sakata hai bhaabhee main jo ghar bhar kee jimmevaaree nibhaata hoon main bhee is dhokhe mein raha"
   ]
  },
  {
   "begin": "239.06",
   "children": [],
   "end": "246.16",
   "id": "f000043",
   "language": "en",
   "lines": [
    "thoda aavesh mein aa gaya vah  main aaj hee ramesh bhaee ko patr likhakar poochhoonga"
   ]
  },
  {
   "begin": "246.16",
   "children": [],
   "end": "249.32",
   "id": "f000044",
   "language": "en",
   "lines": [
    "unhonne yah baat kyon nahin kholee"
   ]
  },
  {
   "begin": "249.32",
   "children": [],
   "end": "253.44",
   "id": "f000045",
   "language": "en",
   "lines": [
    "nahin tum aisa kuchh nahin karoge"
   ]
  },
  {
   "begin": "253.44",
   "children": [],
   "end": "255.68",
   "id": "f000046",
   "language": "en",
   "lines": [
    "kisee se bhee nahin bologe"
   ]
  },
  {
   "begin": "255.68",
   "children": [],
   "end": "258.88",
   "id": "f000047",
   "language": "en",
   "lines": [
    "aur na hee dev se bhee kuchh poochhoge"
   ]
  },
  {
   "begin": "258.88",
   "children": [],
   "end": "264.16",
   "id": "f000048",
   "language": "en",
   "lines": [
    "tumhen meree kasam is baat ko tumase kah diya yahee kaaphee hai"
   ]
  },
  {
   "begin": "264.16",
   "children": [],
   "end": "267.56",
   "id": "f000049",
   "language": "en",
   "lines": [
    "na jaane kaise main himmat kar baithee"
   ]
  },
  {
   "begin": "267.56",
   "children": [],
   "end": "271.88",
   "id": "f000050",
   "language": "en",
   "lines": [
    "aaj mere dil ka bojh halka ho gaya hai"
   ]
  },
  {
   "begin": "271.88",
   "children": [],
   "end": "275.36",
   "id": "f000051",
   "language": "en",
   "lines": [
    "lekin jo kuchh kaha hai bhaee maanakar"
   ]
  },
  {
   "begin": "275.36",
   "children": [],
   "end": "278.66",
   "id": "f000052",
   "language": "en",
   "lines": [
    "isase aage tum chup rahoge"
   ]
  },
  {
   "begin": "278.66",
   "children": [],
   "end": "281.88",
   "id": "f000053",
   "language": "en",
   "lines": [
    "vah bolatee ja rahee thee"
   ]
  },
  {
   "begin": "281.88",
   "children": [],
   "end": "284.68",
   "id": "f000054",
   "language": "en",
   "lines": [
    "mammeedaidee aa rahe hain"
   ]
  },
  {
   "begin": "284.68",
   "children": [],
   "end": "288.36",
   "id": "f000055",
   "language": "en",
   "lines": [
    "dhyaan rahe unhen kuchh na pata lage"
   ]
  },
  {
   "begin": "288.36",
   "children": [],
   "end": "292.52",
   "id": "f000056",
   "language": "en",
   "lines": [
    "mere daidee vaise to bahut shaant prakrti ke hain"
   ]
  },
  {
   "begin": "292.52",
   "children": [],
   "end": "297.22",
   "id": "f000057",
   "language": "en",
   "lines": [
    "par jab unhen gussa aa jae to kuchh ka kuchh kar dete hain"
   ]
  },
  {
   "begin": "297.22",
   "children": [],
   "end": "300.72",
   "id": "f000058",
   "language": "en",
   "lines": [
    "na jaane kya nirnay le baithen"
   ]
  },
  {
   "begin": "300.72",
   "children": [],
   "end": "305.08",
   "id": "f000059",
   "language": "en",
   "lines": [
    "mainne sab apane bhaagy par chhod diya hai"
   ]
  },
  {
   "begin": "305.08",
   "children": [],
   "end": "310.54",
   "id": "f000060",
   "language": "en",
   "lines": [
    "dev ka mere jeevan mein aana likha tha vah ho gaya"
   ]
  },
  {
   "begin": "310.54",
   "children": [],
   "end": "313.36",
   "id": "f000061",
   "language": "en",
   "lines": [
    "ab jo hoga achchha hee hoga"
   ]
  },
  {
   "begin": "313.36",
   "children": [],
   "end": "318.12",
   "id": "f000062",
   "language": "en",
   "lines": [
    "mujhe to lagata bhee nahin ki main kabhee dev ko nahin jaanatee thee"
   ]
  },
  {
   "begin": "318.12",
   "children": [],
   "end": "321.64",
   "id": "f000063",
   "language": "en",
   "lines": [
    "main man se dev ko chaahane lagee hoon"
   ]
  },
  {
   "begin": "321.64",
   "children": [],
   "end": "332.32",
   "id": "f000064",
   "language": "en",
   "lines": [
    "aise mein tum meree itanee hee madad kar do ki mere maataapita jaise hansate hue khushee se aaen vaise hee vah lauten"
   ]
  },
  {
   "begin": "332.32",
   "children": [],
   "end": "338.88",
   "id": "f000065",
   "language": "en",
   "lines": [
    "shesh jo hoga jaisa hoga achchha hee hoga aisa mera vishvaas hai"
   ]
  },
  {
   "begin": "338.88",
   "children": [],
   "end": "349.84",
   "id": "f000066",
   "language": "en",
   "lines": [
    "amit ko lagee veena deveeroop ek shakti is tej raphtaar se chalatee jindagee mein anoothee aurat"
   ]
  },
  {
   "begin": "349.84",
   "children": [],
   "end": "357.28",
   "id": "f000067",
   "language": "en",
   "lines": [
    "jise shaadee ke baad pata laga ki usake saath dhokha hua hai lekin khush hai"
   ]
  },
  {
   "begin": "357.28",
   "children": [],
   "end": "361.88",
   "id": "f000068",
   "language": "en",
   "lines": [
    "apanee khaatir hee nahin apane parivaar kee bhee khaatir"
   ]
  },
  {
   "begin": "361.88",
   "children": [],
   "end": "366.08",
   "id": "f000069",
   "language": "en",
   "lines": [
    "jisane kitanee khushee se sab apana liya hai"
   ]
  },
  {
   "begin": "366.08",
   "children": [],
   "end": "372.56",
   "id": "f000070",
   "language": "en",
   "lines": [
    "abhee das minat pahale tak use jo lag raha tha vah to bilkul asaty tha"
   ]
  },
  {
   "begin": "372.56",
   "children": [],
   "end": "379.68",
   "id": "f000071",
   "language": "en",
   "lines": [
    "usake man mein jo bhaav the vah aur oopar utha gae veena ke svaroop ko"
   ]
  },
  {
   "begin": "379.68",
   "children": [],
   "end": "383.6",
   "id": "f000072",
   "language": "en",
   "lines": [
    "barabas usakee aankhon se aansoo bah nikale"
   ]
  },
  {
   "begin": "383.6",
   "children": [],
   "end": "387.64",
   "id": "f000073",
   "language": "en",
   "lines": [
    "bola vah  main bhee aapaka gunaahagaar hoon"
   ]
  },
  {
   "begin": "387.64",
   "children": [],
   "end": "390.8",
   "id": "f000074",
   "language": "en",
   "lines": [
    "mujhe jo chaahe saja de do"
   ]
  },
  {
   "begin": "390.8",
   "children": [],
   "end": "397.2",
   "id": "f000075",
   "language": "en",
   "lines": [
    "kaisee baat karate ho amit bhaee samajha tabhee man kee baat kah rahee hoon"
   ]
  },
  {
   "begin": "397.2",
   "children": [],
   "end": "399.44",
   "id": "f000076",
   "language": "en",
   "lines": [
    "aisa kuchh bhee mat socho"
   ]
  },
  {
   "begin": "399.44",
   "children": [],
   "end": "403.04",
   "id": "f000077",
   "language": "en",
   "lines": [
    "mujhe tumase kya kisee se gila nahin hai"
   ]
  },
  {
   "begin": "403.04",
   "children": [],
   "end": "408.4",
   "id": "f000078",
   "language": "en",
   "lines": [
    "veena kee baat sunakar amit natamastak ho gaya"
   ]
  },
  {
   "begin": "408.4",
   "children": [],
   "end": "412.92",
   "id": "f000079",
   "language": "en",
   "lines": [
    "baatharoom se paanee chalane kee aavaaz band ho gaee thee"
   ]
  },
  {
   "begin": "412.92",
   "children": [],
   "end": "423.44",
   "id": "f000080",
   "language": "en",
   "lines": [
    "dev ke nikal aane mein chand hee minat the tab veena ne baat samaapt karate hue kaha  dhyaan rakhana jo tumhen kaha hai"
   ]
  },
  {
   "begin": "423.44",
   "children": [],
   "end": "427.82",
   "id": "f000081",
   "language": "en",
   "lines": [
    "aur yah kahakar vah rasoeeghar kee or badh gaee"
   ]
  },
  {
   "begin": "427.82",
   "children": [],
   "end": "431.14",
   "id": "f000082",
   "language": "en",
   "lines": [
    "aur amit apane kamare mein chala gaya"
   ]
  },
  {
   "begin": "431.14",
   "children": [],
   "end": "439.48",
   "id": "f000083",
   "language": "en",
   "lines": [
    "dev ko devarabhaabhee ke beech hue vaartaalaap ka kuchh pata na chala"
   ]
  },
  {
   "begin": "439.48",
   "children": [],
   "end": "445.84",
   "id": "f000084",
   "language": "en",
   "lines": [
    "apane maataapita se milakar veena ka man prasann ho utha"
   ]
  },
  {
   "begin": "445.84",
   "children": [],
   "end": "455.22",
   "id": "f000085",
   "language": "en",
   "lines": [
    "pyaar se maan kee godee mein sir rakh kar aankhen band kar leen usane"
   ]
  },
  {
   "begin": "468.04",
   "children": [],
   "end": "470.08",
   "id": "f000088",
   "language": "en",
   "lines": [
    "maan ka sahaara"
   ]
  },
  {
   "begin": "470.08",
   "children": [],
   "end": "474.44",
   "id": "f000089",
   "language": "en",
   "lines": [

   "children": [],
   "end": "514.84",
   "id": "f000086",
   "language": "en",
   "lines": [
    "sanskaar kya apanee soch jaisee ladakee chaahie"
   ]
  },
  {
   "begin": "514.84",
   "children": [],
   "end": "521.2",
   "id": "f000087",
   "language": "en",
   "lines": [
    "manushy kee jaisee soch hotee hai vaise hee usake sanskaar ban jaate hain"
   ]
  },
  {
   "begin": "521.2",
   "children": [],
   "end": "526.8",
   "id": "f000088",
   "language": "en",
   "lines": [
    "kanaada mein rahate hue bhee usake sanskaar poornatah bhaarateey hee the"
   ]
  },
  {
   "begin": "526.8",
   "children": [],
   "end": "529.8",
   "id": "f000089",
   "language": "en",
   "lines": [
    "poorn roop se shaakaahaaree"
   ]
  },
  {
   "begin": "529.8",
   "children": [],
   "end": "532.32",
   "id": "f000090",
   "language": "en",
   "lines": [
    "mandira se door rahane vaala"
   ]
  },
  {
   "begin": "532.32",
   "children": [],
   "end": "535.44",
   "id": "f000091",
   "language": "en",
   "lines": [
    "sabhee use pandit jee kahate the"
   ]
  },
  {
   "begin": "535.44",
   "children": [],
   "end": "542.44",
   "id": "f000092",
   "language": "en",
   "lines": [
    "aise mein use vahaan kanaada mein goreemem se pataree baithatee nahin dikhatee thee"
   ]
  },
  {
   "begin": "542.44",
   "children": [],
   "end": "548.4",
   "id": "f000093",
   "language": "en",
   "lines": [
    "bhaarateey mool ke logon mein bhee use apanee soch ke anuroop koee janchatee nahin thee"
   ]
  },
  {
   "begin": "548.4",
   "children": [],
   "end": "554.52",
   "id": "f000094",
   "language": "en",
   "lines": [
    "haan ekado pasand aaeen use lekin unhen vah pasand nahin aaya"
   ]
  },
  {
   "begin": "554.52",
   "children": [],
   "end": "562",
   "id": "f000095",
   "language": "en",
   "lines": [
    "kasaratee shareer lambaachauda deeladaul aur us par rang bhee usaka gahara saanvala tha"
   ]
  },
  {
   "begin": "562",
   "children": [],
   "end": "564.2",
   "id": "f000096",
   "language": "en",
   "lines": [
    "aavaaz bhee kadakadaar"
   ]
  },
  {
   "begin": "564.2",
   "children": [],
   "end": "567.52",
   "id": "f000097",
   "language": "en",
   "lines": [
    "kaam ke lie to sambandh sabhee rakhate the"
   ]
  },
  {
   "begin": "567.52",
   "children": [],
   "end": "569.8",
   "id": "f000098",
   "language": "en",
   "lines": [
    "maujamastee ke lie sabhee mil jaate the"
   ]
  },
  {
   "begin": "569.8",
   "children": [],
   "end": "575.16",
   "id": "f000099",
   "language": "en",
   "lines": [
    "lekin ghar basaane kee baat par koee nahin mil paaya"
   ]
  },
  {
   "begin": "575.16",
   "children": [],
   "end": "578.26",
   "id": "f000100",
   "language": "en",
   "lines": [
    "sanyog se milee thee veena"
   ]
  },
  {
   "begin": "578.26",
   "children": [],
   "end": "581.64",
   "id": "f000101",
   "language": "en",
   "lines": [
    "tabhee itane varsh prateeksha karanee padee"
   ]
  },
  {
   "begin": "581.64",
   "children": [],
   "end": "586.88",
   "id": "f000102",
   "language": "en",
   "lines": [
    "ekaek use laga jaise usakee janmabhoomi use bula rahee hai"
   ]
  },
  {
   "begin": "586.88",
   "children": [],
   "end": "591.64",
   "id": "f000103",
   "language": "en",
   "lines": [
    "pandrah saal ka pravaas jaise poora hua usaka"
   ]
  },
  {
   "begin": "591.64",
   "children": [],
   "end": "594.52",
   "id": "f000104",
   "language": "en",
   "lines": [
    "ek maheene ka prograam bana liya"
   ]
  },
  {
   "begin": "594.52",
   "children": [],
   "end": "598.76",
   "id": "f000105",
   "language": "en",
   "lines": [
    "aane se pahale apane kaaailej ke mitr se baat kar lee"
   ]
  },
  {
   "begin": "598.76",
   "children": [],
   "end": "604.64",
   "id": "f000106",
   "language": "en",
   "lines": [
    "ek vahee tha suneel jisase usaka patravyavahaar ho jaata tha"
   ]
  },
  {
   "begin": "604.64",
   "children": [],
   "end": "609.76",
   "id": "f000107",
   "language": "en",
   "lines": [
    "usee ne arun ke aane se pahale akhabaar mein vigyaapan de diya tha"
   ]
  },
  {
   "begin": "609.76",
   "children": [],
   "end": "611.64",
   "id": "f000108",
   "language": "en",
   "lines": [
    "achchhee ladakee ka"
   ]
  },
  {
   "begin": "611.64",
   "children": [],
   "end": "613.6",
   "id": "f000109",
   "language": "en",
   "lines": [
    "umr aade aa rahee thee"
   ]
  },
  {
   "begin": "613.6",
   "children": [],
   "end": "616.04",
   "id": "f000110",
   "language": "en",
   "lines": [
    "usee ko golamol kar gae"
   ]
  },
  {
   "begin": "616.04",
   "children": [],
   "end": "624.24",
   "id": "f000111",
   "language": "en",
   "lines": [
    "achchhee ladakee chaahie chaahe vidhava ho chaahe talaakashuda bas baahar chalakar rahane ko raajee ho"
   ]
  },
  {
   "begin": "624.24",
   "children": [],
   "end": "627.72",
   "id": "f000112",
   "language": "en",
   "lines": [
    "enaaraee kee chhaap bada mahatv rakhatee hai"
   ]
  },
  {
   "begin": "627.72",
   "children": [],
   "end": "632.28",
   "id": "f000113",
   "language": "en",
   "lines": [
    "aur arun to ab bhaarateey mool ka kanaada ka naagarik tha"
   ]
  },
  {
   "begin": "632.28",
   "children": [],
   "end": "635.12",
   "id": "f000114",
   "language": "en",
   "lines": [
    "ek samasya aur bhee thee"
   ]
  },
  {
   "begin": "635.12",
   "children": [],
   "end": "639.04",
   "id": "f000115",
   "language": "en",
   "lines": [
    "desh lautakar maan ke paas rahane kee samasya"
   ]
  },
  {
   "begin": "639.04",
   "children": [],
   "end": "644.36",
   "id": "f000116",
   "language": "en",
   "lines": [
    "vah to ek kamara kiraaye par lekar akelee rahatee thee"
   ]
  },
  {
   "begin": "644.36",
   "children": [],
   "end": "649.56",
   "id": "f000117",
   "language": "en",
   "lines": [
    "pati se kaanoon kee ladaee lad rahee thee pichhale bees varshon se"
   ]
  },
  {
   "begin": "649.56",
   "children": [],
   "end": "656.68",
   "id": "f000118",
   "language": "en",
   "lines": [
    "apane svaabhimaan ko banae rakhane ke lie bhaeeyon se bhee madad nahin maangatee thee"
   ]
  },
  {
   "begin": "656.68",
   "children": [],
   "end": "661.24",
   "id": "f000119",
   "language": "en",
   "lines": [
    "donon khaanadaan dillee ke naameegiraamee raeeson ke the"
   ]
  },
  {
   "begin": "661.24",
   "children": [],
   "end": "665.2",
   "id": "f000120",
   "language": "en",
   "lines": [
    "lekin maan thee ki gumanaamee kee jindagee jeetee thee"
   ]
  },
  {
   "begin": "665.2",
   "children": [],
   "end": "676.4",
   "id": "f000121",
   "language": "en",
   "lines": [
    "aise mein arun ne hee maan ko samajhaaya ki usakee shaadee kee khaatir vah kuchh din tak usake maama ke ghar par hee rah le"
   ]
  },
  {
   "begin": "676.4",
   "children": [],
   "end": "684.52",
   "id": "f000122",
   "language": "en",
   "lines": [
    "maan kaise rahatee hai kaise peechhe ke din bitae isakee soch usake mastishk mein nahin aatee thee"
   ]
  },
  {
   "begin": "684.52",
   "children": [],
   "end": "696.76",
   "id": "f000123",
   "language": "en",
   "lines": [
    "bas kartavy maatr itana samajhaaya tha ki saalachhah maheene mein hajaaradohajaar dolar vah maan ko jaroor bhej deta tha"
   ]
  },
  {
   "begin": "696.76",
   "children": [],
   "end": "704.6",
   "id": "f000124",
   "language": "en",
   "lines": [
    "ab shaadee karane kee ichchha balavatee ho rahee thee to yah sab baaten usake lie maayane rakhatee theen"
   ]
  },
  {
   "begin": "704.6",
   "children": [],
   "end": "711.18",
   "id": "f000125",
   "language": "en",
   "lines": [
    "maan ko to pichhale pandrah saalon mein shaayad pandrahabees hajaar dolar bheje the usane"
   ]
  },
  {
   "begin": "711.18",
   "children": [],
   "end": "718.48",
   "id": "f000126",
   "language": "en",
   "lines": [
    "parantu maamaamaamee ke lie jaroor beshakeematee upahaar laana nahin bhoola"
   ]
  },
  {
   "begin": "718.48",
   "children": [],
   "end": "721.28",
   "id": "f000127",
   "language": "en",
   "lines": [
    "hamaaree sabase badee kamajoree hai"
   ]
  },
  {
   "begin": "721.28",
   "children": [],
   "end": "730.64",
   "id": "f000128",
   "language": "en",
   "lines": [
    "kitane bhee raees kyon na hon ham jab koee bahumooly upahaar laata hai to man baagabaag ho uthata hai"
   ]
  },
  {
   "begin": "730.64",
   "children": [],
   "end": "734.8",
   "id": "f000129",
   "language": "en",
   "lines": [
    "arun jab pahuncha to chitthiyon ka dher laga tha"
   ]
  },
  {
   "begin": "734.8",
   "children": [],
   "end": "741.24",
   "id": "f000130",
   "language": "en",
   "lines": [
    "veena kee chitthee use janchee aur usane turant usaka javaab bhej diya"
   ]
  },
  {
   "begin": "741.24",
   "children": [],
   "end": "747.8",
   "id": "f000131",
   "language": "en",
   "lines": [
    "milane kee ichchha jataee aur aise mein chat manganee pat shaadee ho gaee"
   ]
  },
  {
   "begin": "747.8",
   "children": [],
   "end": "750.7",
   "id": "f000132",
   "language": "en",
   "lines": [
    "arun parinay bandhan mein bandh gaya"
   ]
  },
  {
   "begin": "750.7",
   "children": [],
   "end": "761.02",
   "id": "f000133",
   "language": "en",
   "lines": [
    "veena kee beetee jindagee kee daastaan sunakar use laga yahee vah ladakee hai jo usakee soch ka sambal ban sakatee hai"
   ]
  },
  {
   "begin": "761.02",
   "children": [],
   "end": "767.74",
   "id": "f000134",
   "language": "en",
   "lines": [
    "yahee vah ladakee hai jo usake desh mein usake parivesh mein dhal jaegee"
   ]
  },
  {
   "begin": "767.74",
   "children": [],
   "end": "780.8",
   "id": "f000135",
   "language": "en",
   "lines": [
    "parinayasootr mein bandhane ka samay aa gaya tha usakee jindagee mein aur aise mein chand dinon mein usakee jindagee ke ek nae adhyaay kee shurooaat ho gaee"
   ]
  },
  {
   "begin": "780.8",
   "children": [],
   "end": "786.4",
   "id": "f000136",
   "language": "en",
   "lines": [
    "man baagabaag ho utha veena ko apane jeevan mein paakar"
   ]
  },
  {
   "begin": "786.4",
   "children": [],
   "end": "788.64",
   "id": "f000137",
   "language": "en",
   "lines": [
    "arun kee tandra bhang huee"
   ]
  },
  {
   "begin": "794.14",
   "children": [],
   "end": "798.96",
   "id": "f000140",
   "language": "en",
   "lines": [
    "veena ko paakar vah bhaavavibhor ho utha"
   ]
  },
  {
   "begin": "798.96",
   "children": [],
   "end": "803.12",
   "id": "f000141",
   "language": "en",
   "lines": [
    "jeevan mein usake naee bahaaren aa gaeen"
   ]
  },
  {
   "begin": "803.12",
   "children": [],
   "end": "813.76",
   "id": "f000142",
   "language": "en",
   "lines": [
    "aisa ehasaas use pahale kabhee nahin hua tha veena ko apanee baahon mein lekar vah mastamadahosh ho gaya"
   ]
  },
  {
   "begin": "813.76",
   "children": [],
   "end": "819.16",
   "id": "f000143",
   "language": "en",
   "lines": [
    "raat ke do baj rahe the jab veena kee aankh khuleen"
   ]
  },
  {
   "begin": "819.16",
   "children": [],
   "end": "823.2",
   "id": "f000144",
   "language": "en",
   "lines": [
    "usake pahaloo mein arun soya hua tha"
   ]
  },
  {
   "begin": "823.2",
   "children": [],
   "end": "826",
   "id": "f000145",
   "language": "en",
   "lines": [
    "ek najar arun par daalee usane"
   ]
  },
  {
   "begin": "826",
   "children": [],
   "end": "832.52",
   "id": "f000146",
   "language": "en",
   "lines": [
    "phir khud ko dekhane kee cheshta kee usane sharma gaee apane ko dekhakar"
   ]
  },
  {
   "begin": "832.52",
   "children": [],
   "end": "836.48",
   "id": "f000147",
   "language": "en",
   "lines": [
    "kamare mein halkee see roshanee thee"
   ]
  },
  {
   "begin": "836.48",
   "children": [],
   "end": "842.1",
   "id": "f000148",
   "language": "en",
   "lines": [
    "arun kee baajoo dheere se hataakar vah karavat badal kar let gaee"
   ]
  },
  {
   "begin": "842.1",
   "children": [],
   "end": "851.16",
   "id": "f000149",
   "language": "en",
   "lines": [
    "ab usakee aankhon ke saamane khidakee thee aur khidakee ke baahar use taaron bhara aakaash dikh raha tha"
   ]
  },
  {
   "begin": "851.16",
   "children": [],
   "end": "859.54",
   "id": "f000150",
   "language": "en",
   "lines": [
    "veena kee najar aur nahin jhatakee unheen taaron mein se ek taare par thithak kar rah gaee"
   ]
  },
  {
   "begin": "859.54",
   "children": [],
   "end": "861.26",
   "id": "f000151",
   "language": "en",
   "lines": [
    "bahut chamak raha tha"
   ]
  },
  {
   "begin": "861.26",
   "children": [],
   "end": "862.48",
   "id": "f000152",
   "language": "en",
   "lines": [
    "vah taara"
   ]
  },
  {
   "begin": "862.48",
   "children": [],
   "end": "868.42",
   "id": "f000153",
   "language": "en",
   "lines": [
    "use vah taara apanee aankhon ke nikat aata dikhaee diya"
   ]
  },
  {
   "begin": "868.42",
   "children": [],
   "end": "875.6",
   "id": "f000154",
   "language": "en",
   "lines": [
    "bahut dinon baad use laga yah raat achchhee hai yah taare usake saathee hain"
   ]
  },
  {
   "begin": "875.6",
   "children": [],
   "end": "886.12",
   "id": "f000155",
   "language": "en",
   "lines": [
    "aur sabase alag use yah sochakar bhee achchha laga ki in taaron ko dekhakar ab vah muskara sakatee hai"
   ]
  },
  {
   "begin": "886.12",
   "children": [],
   "end": "892.1",
   "id": "f000156",
   "language": "en",
   "lines": [
    "vah muskaraee bhee aur phir se ek najar arun par daalee"
   ]
  },
  {
   "begin": "892.1",
   "children": [],
   "end": "895.72",
   "id": "f000157",
   "language": "en",
   "lines": [
    "vah ekatak use dekhane lagee"
   ]
  },
  {
   "begin": "895.72",
   "children": [],
   "end": "897.72",
   "id": "f000158",
   "language": "en",
   "lines": [
    "arun so raha tha"
   ]
  },
  {
   "begin": "897.72",
   "children": [],
   "end": "900.8",
   "id": "f000159",
   "language": "en",
   "lines": [
    "saanson kee aavaaz aa rahee thee"
   ]
  },
  {
   "begin": "900.8",
   "children": [],
   "end": "903.96",
   "id": "f000160",
   "language": "en",
   "lines": [
    "shareer se bhaaree tha arun"
   ]
  },
  {
   "begin": "903.96",
   "children": [],
   "end": "905.84",
   "id": "f000161",
   "language": "en",
   "lines": [
    "saanvala tha"
   ]
  },
  {
   "begin": "905.84",
   "children": [],
   "end": "911.24",
   "id": "f000162",
   "language": "en",
   "lines": [
    "binaachashme ke is samay chehara veena ko anjaan sa laga"
   ]
  },
  {
   "begin": "911.24",
   "children": [],
   "end": "913.12",
   "id": "f000163",
   "language": "en",
   "lines": [
    "aakarshanaheen"
   ]
  },
  {
   "begin": "913.12",
   "children": [],
   "end": "920.76",
   "id": "f000164",
   "language": "en",
   "lines": [
    "ekaek na keval veena kee muskuraahat lupt ho gaee balki vah udaas ho gaee"
   ]
  },
  {
   "begin": "920.76",
   "children": [],
   "end": "928.42",
   "id": "f000165",
   "language": "en",
   "lines": [
    "bhool gaee ki abhee kuchh der pahale isee vyakti ne use shaareerikasukh diya hai"
   ]
  },
  {
   "begin": "928.42",
   "children": [],
   "end": "931.52",
   "id": "f000166",
   "language": "en",
   "lines": [
    "arun use ajanabee sa lagane laga"
   ]
  },
  {
   "begin": "931.52",
   "children": [],
   "end": "935.48",
   "id": "f000167",
   "language": "en",
   "lines": [
    "veena ne arun se apanee drshti hata lee"
   ]
  },
  {
   "begin": "935.48",
   "children": [],
   "end": "941.32",
   "id": "f000168",
   "language": "en",
   "lines": [
    "phir baahar kee or dekhane lagee aur use vahee taara phir dikh gaya"
   ]
  },
  {
   "begin": "941.32",
   "children": [],
   "end": "945.28",
   "id": "f000169",
   "language": "en",
   "lines": [
    "vah use aur adhik timatimaata laga"
   ]
  },
  {
   "begin": "945.28",
   "children": [],
   "end": "956.62",
   "id": "f000170",
   "language": "en",
   "lines": [
    "jaise us taare ne aakaash ke dochaar taaron ka tej churaakar apane aapako aur adhik aakarshak bana liya ho"
   ]
  },
  {
   "begin": "956.62",
   "children": [],
   "end": "962.56",
   "id": "f000171",
   "language": "en",
   "lines": [
    "vah taara dheeredheere use aakaar mein badalata dikhaee diya"
   ]
  },
  {
   "begin": "962.56",
   "children": [],
   "end": "964.8",
   "id": "f000172",
   "language": "en",
   "lines": [
    "o ye kya"
   ]
  },
  {
   "begin": "964.8",
   "children": [],
   "end": "967.48",
   "id": "f000173",

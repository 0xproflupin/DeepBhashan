   "end": "366.64",
   "id": "f000091",
   "language": "en",
   "lines": [
    "aap bataie aapake pandit ne koee aur shubh mahoort bataaya hai"
   ]
  },
  {
   "begin": "366.64",
   "children": [],
   "end": "370.32",
   "id": "f000092",
   "language": "en",
   "lines": [
    "aapane poochh liya vahee kaaphee hai"
   ]
  },
  {
   "begin": "370.32",
   "children": [],
   "end": "371.8",
   "id": "f000093",
   "language": "en",
   "lines": [
    "pannaalaal ne kaha"
   ]
  },
  {
   "begin": "371.8",
   "children": [],
   "end": "380",
   "id": "f000094",
   "language": "en",
   "lines": [
    "is par ramesh ne kaha tab theek hai nau pharavaree ko ham log shaam tak pahunch jaenge"
   ]
  },
  {
   "begin": "380",
   "children": [],
   "end": "384.44",
   "id": "f000095",
   "language": "en",
   "lines": [
    "raat phere dilavaakar das kee subah vaapis aa jaenge"
   ]
  },
  {
   "begin": "384.44",
   "children": [],
   "end": "384.44",
   "id": "f000096",
   "language": "en",
   "lines": [
    ""
   ]
  },
  {
   "begin": "384.44",
   "children": [],
   "end": "393.02",
   "id": "f000097",
   "language": "en",
   "lines": [
    "tab nirmala ne kaha krshna se bahan jee aapake reetirivaajon ka hamen kuchh adhik nahin pata"
   ]
  },
  {
   "begin": "393.02",
   "children": [],
   "end": "395.22",
   "id": "f000098",
   "language": "en",
   "lines": [
    "thoda kuchh bata deejie"
   ]
  },
  {
   "begin": "395.22",
   "children": [],
   "end": "401.72",
   "id": "f000099",
   "language": "en",
   "lines": [
    "abhee krshna kuchh kahatee ki dev bol utha koee reetirivaaj nahin hain"
   ]
  },
  {
   "begin": "401.72",
   "children": [],
   "end": "408.48",
   "id": "f000100",
   "language": "en",
   "lines": [
    "bas ham paanch log aaenge shaanti se aur hamen kuchh chaahie bhee nahin"
   ]
  },
  {
   "begin": "408.48",
   "children": [],
   "end": "408.48",
   "id": "f000101",
   "language": "en",
   "lines": [
    ""
   ]
  },
  {
   "begin": "408.48",
   "children": [],
   "end": "410.68",
   "id": "f000102",
   "language": "en",
   "lines": [
    "shaanti se nahin beta"
   ]
  },
  {
   "begin": "410.68",
   "children": [],
   "end": "417.08",
   "id": "f000103",
   "language": "en",
   "lines": [
    "deharaadoon mein jab se hamane ghar banaaya hai yah hamaare ghar kee pahalee shaadee hai"
   ]
  },
  {
   "begin": "417.08",
   "children": [],
   "end": "421.24",
   "id": "f000104",
   "language": "en",
   "lines": [
    "suneeta kee shaadee hamane himaachal mein hee kee thee"
   ]
  },
  {
   "begin": "421.24",
   "children": [],
   "end": "423.72",
   "id": "f000105",
   "language": "en",
   "lines": [
    "saaree rishtedaaree jo vahaan hai"
   ]
  },
  {
   "begin": "423.72",
   "children": [],
   "end": "427.84",
   "id": "f000106",
   "language": "en",
   "lines": [
    "ab veena kee shaadee kaise sab chuppee se ho jae"
   ]
  },
  {
   "begin": "427.84",
   "children": [],
   "end": "430.16",
   "id": "f000107",
   "language": "en",
   "lines": [
    "yah theek nahin lagata"
   ]
  },
  {
   "begin": "430.16",
   "children": [],
   "end": "433.64",
   "id": "f000108",
   "language": "en",
   "lines": [
    "aap ghar ke log hee aaen koee baat nahin"
   ]
  },
  {
   "begin": "433.64",
   "children": [],
   "end": "437.2",
   "id": "f000109",
   "language": "en",
   "lines": [
    "vaise achchha lagata ki baaraat aatee"
   ]
  },
  {
   "begin": "437.2",
   "children": [],
   "end": "439.24",
   "id": "f000110",
   "language": "en",
   "lines": [
    "phir bhee meree praarthana hai baindabaaja aae"
   ]
  },
  {
   "begin": "439.24",
   "children": [],
   "end": "441.78",
   "id": "f000111",
   "language": "en",
   "lines": [
    ""
   ]
  },
  {
   "begin": "441.78",
   "children": [],
   "end": "444.5",
   "id": "f000112",
   "language": "en",
   "lines": [
    "chalie aapakee baat rahegee"
   ]
  },
  {
   "begin": "444.5",
   "children": [],
   "end": "447.2",
   "id": "f000113",
   "language": "en",
   "lines": [
    "shesh hamen kuchh nahin chaahie"
   ]
  },
  {
   "begin": "447.2",
   "children": [],
   "end": "450.7",
   "id": "f000114",
   "language": "en",
   "lines": [
    "ramesh kumaar ne nirnayaatmak svar mein kah diya"
   ]
  },
  {
   "begin": "450.7",
   "children": [],
   "end": "453.88",
   "id": "f000115",
   "language": "en",
   "lines": [
    "aur hamaaree koee maang nahin hai"
   ]
  },
  {
   "begin": "453.88",
   "children": [],
   "end": "457.6",
   "id": "f000116",
   "language": "en",
   "lines": [
    "aapane jo dena hai apanee betee ko dena hai"
   ]
  },
  {
   "begin": "457.6",
   "children": [],
   "end": "463.48",
   "id": "f000117",
   "language": "en",
   "lines": [
    "krshna ne kaha vaise hamaare mein sab bhaeebahan ke kapade hote hain"
   ]
  },
  {
   "begin": "463.48",
   "children": [],
   "end": "473.36",
   "id": "f000118",
   "language": "en",
   "lines": [
    "sabhee rishtedaaron abhee krshna isase aage kuchh kahatee ki dev ne tanik aavesh mein kaha  kuchh nahin hota hamaare mein"
   ]
  },
  {
   "begin": "473.36",
   "children": [],
   "end": "478.28",
   "id": "f000119",
   "language": "en",
   "lines": [
    "na bhaeebahan ka na mera na hee kisee rishtedaar ka"
   ]
  },
  {
   "begin": "478.28",
   "children": [],
   "end": "478.28",
   "id": "f000120",
   "language": "en",
   "lines": [
    ""
   ]
  },
  {
   "begin": "478.28",
   "children": [],
   "end": "483.68",
   "id": "f000121",
   "language": "en",
   "lines": [
    "phir maan ke aamukh hokar bola  mammee aap samajhatee kyon nahin mujhe"
   ]
  },
  {
   "begin": "483.68",
   "children": [],
   "end": "488.44",
   "id": "f000122",
   "language": "en",
   "lines": [
    "mujhe koee bhee lenaden dikhaava pasand nahin hai"
   ]
  },
  {
   "begin": "488.44",
   "children": [],
   "end": "488.44",
   "id": "f000123",
   "language": "en",
   "lines": [
    ""
   ]
  },
  {
   "begin": "488.44",
   "children": [],
   "end": "501.52",
   "id": "f000124",
   "language": "en",
   "lines": [
    "dev kee baat sunakar pannaalaal va nirmala ko mahasoos hua ki dev ke shabdon mein kathorata thee adiga the shabd usake  jinhen koee na badal saka"
   ]
  },
  {
   "begin": "501.52",
   "children": [],
   "end": "507.64",
   "id": "f000125",
   "language": "en",
   "lines": [
    "aur maan ab khisiyaasee gaee bolee mera aisa to matalab na tha"
   ]
  },
  {
   "begin": "507.64",
   "children": [],
   "end": "511.64",
   "id": "f000126",
   "language": "en",
   "lines": [
    "bahan jee ne poochha mainne kaha"
   ]
  },
  {
   "begin": "511.24",
   "children": [],
   "end": "515.76",
   "id": "f000128",
   "language": "en",
   "lines": [
    "aisa kahana aur jataana ek hee baat hai"
   ]
  },
  {
   "begin": "515.76",
   "children": [],
   "end": "519.96",
   "id": "f000129",
   "language": "en",
   "lines": [
    "maan in baaton ko rishton ke beech na laie"
   ]
  },
  {
   "begin": "519.96",
   "children": [],
   "end": "528.4",
   "id": "f000130",
   "language": "en",
   "lines": [
    "dev ne kuchh is tarah kaha ki jaise ishaara kiya ho usane is baat ko yaheen samaapt karane ka"
   ]
  },
  {
   "begin": "528.4",
   "children": [],
   "end": "535.64",
   "id": "f000131",
   "language": "en",
   "lines": [
    "tab ramesh kumaar ne samajhadaaree dikhaee aur maalatee ko kaha bhee khaane kee taiyaaree karo"
   ]
  },
  {
   "begin": "535.64",
   "children": [],
   "end": "535.64",
   "id": "f000132",
   "language": "en",
   "lines": [
    ""
   ]
  },
  {
   "begin": "535.64",
   "children": [],
   "end": "539.36",
   "id": "f000133",
   "language": "en",
   "lines": [
    "naheennaheen ramesh jee ham khaana nahin khaenge"
   ]
  },
  {
   "begin": "539.36",
   "children": [],
   "end": "539.36",
   "id": "f000134",
   "language": "en",
   "lines": [
    ""
   ]
  },
  {
   "begin": "539.36",
   "children": [],
   "end": "541.36",
   "id": "f000135",
   "language": "en",
   "lines": [
    "khaana kyon nahin khaenge"
   ]
  },
  {
   "begin": "541.36",
   "children": [],
   "end": "545.08",
   "id": "f000136",
   "language": "en",
   "lines": [
    "aap to in puraanee vichaar dhaaraon ko na apanaie"
   ]
  },
  {
   "begin": "545.08",
   "children": [],
   "end": "545.12",
   "id": "f000137",
   "language": "en",
   "lines": [
    ""
   ]
  },
  {
   "begin": "545.12",
   "children": [],
   "end": "547.56",
   "id": "f000138",
   "language": "en",
   "lines": [
    "nahin aisee baat nahin"
   ]
  },
  {
   "begin": "547.56",
   "children": [],
   "end": "547.56",
   "id": "f000139",
   "language": "en",
   "lines": [
    ""
   ]
  },
  {
   "begin": "547.56",
   "children": [],
   "end": "549.68",
   "id": "f000140",
   "language": "en",
   "lines": [
    "to phir sankoch kaahe ka"
   ]
  },
  {
   "begin": "549.68",
   "children": [],
   "end": "555.28",
   "id": "f000141",
   "language": "en",
   "lines": [
    "ab yadi aapaka ghar hamaara hai to hamaara ghar bhee aap hee ka hai"
   ]
  },
  {
   "begin": "555.28",
   "children": [],
   "end": "558.84",
   "id": "f000142",
   "language": "en",
   "lines": [
    "is rishte mein sankoch nahin hona chaahie"
   ]
  },
  {
   "begin": "558.84",
   "children": [],
   "end": "558.88",
   "id": "f000143",
   "language": "en",
   "lines": [
    ""
   ]
  },
  {
   "begin": "558.88",
   "children": [],
   "end": "565.28",
   "id": "f000144",
   "language": "en",
   "lines": [
    "khaane ke pashchaat pannaalaal va nirmala deharaadoon lautana chaahate the"
   ]
  },
  {
   "begin": "565.28",
   "children": [],
   "end": "569.16",
   "id": "f000145",
   "language": "en",
   "lines": [
    "lekin sabhee ne aagrahapoorvak unhen rok liya"
   ]
  },
  {
   "begin": "569.16",
   "children": [],
   "end": "575.64",
   "id": "f000146",
   "language": "en",
   "lines": [
    "nirmala kee bhee chaah thee ki dillee aaee hai to bachchon ke lie kuchh letee jae"
   ]
  },
  {
   "begin": "575.64",
   "children": [],
   "end": "580.08",
   "id": "f000147",
   "language": "en",
   "lines": [
    "usake man kee baat jaise amit ne padh lee ho"
   ]
  },
  {
   "begin": "580.08",
   "children": [],
   "end": "590.08",
   "id": "f000148",
   "language": "en",
   "lines": [
    "shaam kee chaay ke dauraan usee ne kaha tha  aantee aap bor ho rahee hain man hai to chalie aapako baajaar tak ghuma laoon"
   ]
  },
  {
   "begin": "590.08",
   "children": [],
   "end": "592.28",
   "id": "f000149",
   "language": "en",
   "lines": [
    "nirmala ne dekha usakee or"
   ]
  },
  {
   "begin": "592.28",
   "children": [],
   "end": "593.2",
   "id": "f000150",
   "language": "en",
   "lines": [
    "khush huee"
   ]
  },
  {
   "begin": "593.2",
   "children": [],
   "end": "597.24",
   "id": "f000151",
   "language": "en",
   "lines": [
    "achchha ladaka hai usake man kee baat samajh gaya hai"
   ]
  },
  {
   "begin": "597.24",
   "children": [],
   "end": "600.6",
   "id": "f000152",
   "language": "en",
   "lines": [
    "vaise vah sankochee svabhaav kee thee"
   ]
  },
  {
   "begin": "600.6",
   "children": [],
   "end": "605.32",
   "id": "f000153",
   "language": "en",
   "lines": [
    "tabhee to krshna se adhik baat nahin kar sakee thee abhee tak"
   ]
  },
  {
   "begin": "605.32",
   "children": [],
   "end": "612.04",
   "id": "f000154",
   "language": "en",
   "lines": [
    "ghar se baahar aakar use baaten karane kee apeksha baaten sunane mein maja aata tha"
   ]
  },
  {
   "begin": "612.04",
   "children": [],
   "end": "625.16",
   "id": "f000155",
   "language": "en",
   "lines": [
    "jab bhee ghar se baahar jaatee thee dherasee baaten sunakar hee aatee thee aur ghar lautakar sabhee bachchon ko ek kahaanee kee tarah piro kar vahee baaten sunaatee thee"
   ]
  },
  {
   "begin": "625.16",
   "children": [],
   "end": "630.84",
   "id": "f000156",
   "language": "en",
   "lines": [
    "tabhee sabhee bachche maan kee prateeksha mein aankhen bichhae baithe rahate the"
   ]
  },
  {
   "begin": "630.84",
   "children": [],
   "end": "635.56",
   "id": "f000157",
   "language": "en",
   "lines": [
    "maan ke bina ghar sabhee ko soona lagata tha"
   ]
  },
  {
   "begin": "635.56",
   "children": [],
   "end": "641.2",
   "id": "f000158",
   "language": "en",
   "lines": [
    "jaana to chaahatee hoon beta yadi tumhen takaleeph na ho"
   ]
  },
  {
   "begin": "641.2",
   "children": [],
   "end": "645.48",
   "id": "f000159",
   "language": "en",
   "lines": [
    "kuchh sankoch bhare lahaje mein kaha nirmala ne"
   ]
  },
  {
   "begin": "645.48",
   "children": [],
   "end": "648",
   "id": "f000160",
   "language": "en",
   "lines": [
    "to phir uthie taiyaar ho jaeee"
   ]
  },
  {
   "begin": "648",
   "children": [],
   "end": "648.92",
   "id": "f000161",
   "language": "en",
   "lines": [
    "abhee chalate hain"
   ]
  },
  {
   "begin": "648.92",
   "children": [],
   "end": "653.5",
   "id": "f000162",
   "language": "en",
   "lines": [
    "amit ko jaise munh maangee muraad mil gaee"
   ]
  },
  {
   "begin": "653.5",
   "children": [],
   "end": "657.8",
   "id": "f000163",
   "language": "en",
   "lines": [
    "lekin krshna ko yah sab achchha nahin laga"
   ]
  },
  {
   "begin": "657.8",
   "children": [],
   "end": "663.64",
   "id": "f000164",
   "language": "en",
   "lines": [
    "shaayad vah chaahatee thee ki nirmala ke saath vah baajaar jae"
   ]
  },
  {
   "begin": "663.64",
   "children": [],
   "end": "670.76",
   "id": "f000165",
   "language": "en",
   "lines": [
    "lekin vah aisa kuchh nahin kahana chaahatee thee ki jisase dev naaraaj ho jae"
   ]
  },
  {
   "begin": "670.76",
   "children": [],
   "end": "680.96",
   "id": "f000166",
   "language": "en",
   "lines": [
    "dev ka svabhaav hee kuchh aisa tha ki shuroo se hee ghar mein usase ya usake saamane koee bhee adhik nahin bolata tha"
   ]
  },
  {
   "begin": "680.96",
   "children": [],
   "end": "686.28",
   "id": "f000167",
   "language": "en",
   "lines": [
    "aise mein krshna ko manamasos kar rah jaana pada"
   ]
  },
  {
   "begin": "686.28",
   "children": [],
   "end": "695.06",
   "id": "f000168",
   "language": "en",
   "lines": [
    "amit kuchh hee der mein taiyaar hokar apanee motar saikil par nirmala ko bitha kar kamalaanagar kee taraph chal diya"
   ]
  },
  {
   "begin": "695.06",
   "children": [],
   "end": "699.52",
   "id": "f000169",
   "language": "en",
   "lines": [
    "vah nirmala par apana achchha prabhaav daalana chaah raha tha"
   ]
  },
  {
   "begin": "699.52",
   "children": [],
   "end": "707.64",
   "id": "f000170",
   "language": "en",
   "lines": [
    "usakee haardik ichchha thee ki nirmala usase usake baare mein kuchh poochhe usake vyaapaar ke baare mein poochhe"
   ]
  },
  {
   "begin": "707.64",
   "children": [],
   "end": "710.92",
   "id": "f000171",
   "language": "en",
   "lines": [
    "taaki vah use prabhaavit kar sake"
   ]
  },
  {
   "begin": "710.92",
   "children": [],
   "end": "713.92",
   "id": "f000172",
   "language": "en",
   "lines": [
    "lekin aisa kuchh nahin hua"
   ]
  },
  {
   "begin": "713.92",
   "children": [],
   "end": "721.68",
   "id": "f000173",
   "language": "en",
   "lines": [
    "nirmala baajaar mein ghoomate hue use bataatee rahee ki vah kya kuchh lena chaah rahee hai"
   ]
  },
  {
   "begin": "721.68",
   "children": [],
   "end": "723.68",
   "id": "f000174",
   "language": "en",
   "lines": [
    "amit usakee jaroorat"
   ]
  },
  {
   "begin": "723.68",
   "children": [],
   "end": "727.96",
   "id": "f000175",
   "language": "en",
   "lines": [
    "ke anusaar kisee na kisee dukaan par use lekar ja raha tha"
   ]
  },
  {
   "begin": "727.96",
   "children": [],
   "end": "740.38",
   "id": "f000176",
   "language": "en",
   "lines": [
    "nirmala ne priya ke lie ek svetar rajanee ke lie ek shaal geeta ke lie ek jodee saindal aur prem ke lie ek silk kee kameej khareedee"
   ]
  },
  {
   "begin": "740.38",
   "children": [],
   "end": "747.92",
   "id": "f000177",
   "language": "en",
   "lines": [
    "amit ne use priya kee svetar chunane mein adhik ruchee dikhaee nirmala ne bhee aisa mahasoos kiya"
   ]
  },
  {
   "begin": "747.92",
   "children": [],
   "end": "751.96",
   "id": "f000178",
   "language": "en",
   "lines": [
    "sab hans rahe the khilakhilaakar"
   ]
  },
  {
   "begin": "751.96",
   "children": [],
   "end": "755.78",
   "id": "f000179",
   "language": "en",
   "lines": [
    "ramesh dev pramod aur amit"
   ]
  },
  {
   "begin": "755.78",
   "children": [],
   "end": "758.88",
   "id": "f000180",
   "language": "en",
   "lines": [
    "draeeng room mein baithe the sabhee"
   ]
  },
  {
   "begin": "758.88",
   "children": [],
   "end": "762.16",
   "id": "f000181",
   "language": "en",
   "lines": [
    "saamane sabhee ke gilaas pade the"
   ]
  },
  {
   "begin": "762.16",
   "children": [],
   "end": "763.28",
   "id": "f000182",
   "language": "en",
   "lines": [
    "vhiskee se bhare"
   ]
  },
  {
   "begin": "763.28",
   "children": [],
   "end": "768.72",
   "id": "f000183",
   "language": "en",
   "lines": [
    "dev ke vivaah ke shubh mahoort kee khushee mana rahe the ve sab"
   ]
  },
  {
   "begin": "768.72",
   "children": [],
   "end": "774",
   "id": "f000184",
   "language": "en",
   "lines": [
    "dev ko mahasoos ho raha tha usake tanaav kee ghadiyaan"
   ]
  },
  {
   "begin": "774",
   "children": [],
   "end": "777.72",
   "id": "f000185",
   "language": "en",
   "lines": [
    "akelepan ke sabhee kshan ab samaapt hone ja rahe hain"
   ]
  },
  {
   "begin": "777.72",
   "children": [],
   "end": "790.36",
   "id": "f000186",
   "language": "en",
   "lines": [
    "vyaavahaarikajeevan mein jojo mushkilen usake saamane theen ve sab ab use lag raha tha na ke baraabar rah jaengee ek saathee ke aa jaane se"
   ]
  },
  {
   "begin": "790.36",
   "children": [],
   "end": "796.12",
   "id": "f000187",
   "language": "en",
   "lines": [
    "isee khushee mein aaj usane bhee jee bhar ke peene kee thaan rakhee thee"
   ]
  },
  {
   "begin": "796.12",
   "children": [],
   "end": "801.12",
   "id": "f000188",
   "language": "en",
   "lines": [
    "usane hee kya sabhee aaj dher saaree peena chaah rahe the"
   ]
  },
  {
   "begin": "801.12",
   "children": [],
   "end": "811.32",
   "id": "f000189",
   "language": "en",
   "lines": [
    "baahar mausam mein thandak thee aur bheetar baithe in sabhee ke shareeron ko sharaab ke ghoont garm kie ja rahe the"
   ]
  },
  {
   "begin": "811.32",
   "children": [],
   "end": "816.2",
   "id": "f000190",
   "language": "en",
   "lines": [
    "kamare ke ek kone mein rikaardapleyar baj raha tha"
   ]
  },
  {
   "begin": "816.2",
   "children": [],
   "end": "824",
   "id": "f000191",
   "language": "en",
   "lines": [
    "amit ka aadha dhyaan sabakee baaton mein aur aadha dhyaan rikordapleyar kee or tha"
   ]
  },
  {
   "begin": "824",
   "children": [],
   "end": "832.48",
   "id": "f000192",
   "language": "en",
   "lines": [
    "jabajab rikaard samaapt hota vah jhat se uthakar badal deta tha apanee pasand ka sunane ke lie"
   ]
  },
  {
   "begin": "832.48",
   "children": [],
   "end": "843.14",
   "id": "f000193",
   "language": "en",
   "lines": [
    "is baar jab rikord khatm hua to pramod bola amit se  ab ye halke gaanegajal chhodo amit koee bhadakeela gaana lagao"
   ]
  },
  {
   "begin": "843.14",
   "children": [],
   "end": "848.08",
   "id": "f000194",
   "language": "en",
   "lines": [
    "vo hai na tumhaare paas rikaard irapshan vahee lagao"
   ]
  },
  {
   "begin": "848.08",
   "children": [],
   "end": "854.92",
   "id": "f000195",
   "language": "en",
   "lines": [
    "pramod nashe mein jhoom raha tha van ve tikit main naachoonga aaj"
   ]
  },
  {
   "begin": "854.92",
   "children": [],
   "end": "864.94",
   "id": "f000196",
   "language": "en",
   "lines": [
    "aur amit ne abhee rikaard lagaaya bhee na tha ki vah apanee kursee se khada hokar apane man kee dhun par thirakane laga tha"
   ]
  },
  {
   "begin": "864.94",
   "children": [],
   "end": "870.68",
   "id": "f000197",
   "language": "en",
   "lines": [
    "amit ne raik se usakee pasand ka rikaard nikaal kar laga diya"
   ]
  },
  {
   "begin": "870.68",
   "children": [],
   "end": "875.2",
   "id": "f000198",
   "language": "en",
   "lines": [
    "phaast karo phaast halkee aavaaz se maja nahin aaega"
   ]
  },
  {
   "begin": "875.2",
   "children": [],
   "end": "878.8",
   "id": "f000199",
   "language": "en",
   "lines": [
    "aur amit ne aavaaz bhee tej kar dee"
   ]
  },
  {
   "begin": "878.8",
   "children": [],
   "end": "882.16",
   "id": "f000200",
   "language": "en",
   "lines": [
    "tab pramod jorajor se naachane laga tha"
   ]
  },
  {
   "begin": "882.16",
   "children": [],
   "end": "886.12",
   "id": "f000201",
   "language": "en",
   "lines": [
    "vah naach kya raha tha jhoom raha tha"
   ]
  },
  {
   "begin": "886.12",
   "children": [],
   "end": "889.44",
   "id": "f000202",
   "language": "en",
   "lines": [
    "lekin isee mein use aanand aa raha tha"
   ]
  },
  {
   "begin": "889.44",
   "children": [],
   "end": "893.84",
   "id": "f000203",
   "language": "en",
   "lines": [
    "shesh sabhee use aisa karate dekh kar hans rahe the"
   ]
  },
  {
   "begin": "893.84",
   "children": [],
   "end": "899",
   "id": "f000204",
   "language": "en",
   "lines": [
    "tabhee pramod ne amit kee taraph badhakar use bhee beech mein kheencha"
   ]
  },
  {
   "begin": "899",
   "children": [],
   "end": "902.62",
   "id": "f000205",
   "language": "en",
   "lines": [
    "chalo aao tum bhee mere saath daans karo"
   ]
  },
  {
   "begin": "902.62",
   "children": [],
   "end": "906.08",
   "id": "f000206",
   "language": "en",
   "lines": [
    "are itanee badee khushee ka din hai aaj"
   ]
  },
  {
   "begin": "906.08",
   "children": [],
   "end": "908.88",
   "id": "f000207",
   "language": "en",
   "lines": [
    "doktar saahab kee shaadee hai"
   ]
  },
  {
   "begin": "908.88",
   "children": [],
   "end": "911.72",
   "id": "f000208",
   "language": "en",
   "lines": [
    "aur vah jor se naachane laga"
   ]
  },
  {
   "begin": "911.72",
   "children": [],
   "end": "917.44",
   "id": "f000209",
   "language": "en",
   "lines": [
    "haahoo kee aavaaz nikaalate dekh kar pramod par sabhee hans rahe the"
   ]
  },
  {
   "begin": "917.44",
   "children": [],
   "end": "922.52",
   "id": "f000210",
   "language": "en",
   "lines": [
    "amit bhee usake saath usake saamane khada naach raha tha"
   ]
  },
  {
   "begin": "922.52",
   "children": [],
   "end": "925.24",
   "id": "f000211",
   "language": "en",
   "lines": [
    "kuchh der tak aise hee chalata raha"
   ]
  },
  {
   "begin": "925.24",
   "children": [],
   "end": "934.04",
   "id": "f000212",
   "language": "en",
   "lines": [
    "van ve tikit  samaapt hote hee ramesh ne sueen oopar utha dee aur kaha pramod se  maja aa gaya"
   ]
  },
  {
   "begin": "934.04",
   "children": [],
   "end": "937.72",
   "id": "f000213",
   "language": "en",
   "lines": [
    "aaj pramod tumhaara badhiya mood dekhakar"
   ]
  },
  {
   "begin": "937.72",
   "children": [],
   "end": "940.48",
   "id": "f000214",
   "language": "en",
   "lines": [
    "vaise kya khyaal hai ab khaane ke baare mein"
   ]
  },
  {
   "begin": "940.48",
   "children": [],
   "end": "944.04",
   "id": "f000215",
   "language": "en",
   "lines": [
    "khaana khaenge sabhee khaenge"
   ]
  },
  {
   "begin": "944.04",
   "children": [],
   "end": "945.76",
   "id": "f000216",
   "language": "en",
   "lines": [
    "pahale naacho sabhee"
   ]
  },
  {
   "begin": "945.76",
   "children": [],
   "end": "952.36",
   "id": "f000217",
   "language": "en",
   "lines": [
    "pramod rikaard kee aavaaz band ho jaane ke baavajood naach raha tha"
   ]
  },
  {
   "begin": "952.36",
   "children": [],
   "end": "955.6",
   "id": "f000218",
   "language": "en",
   "lines": [
    "naachenge bhaee pahale khaana kha lete hain"
   ]
  },
  {
   "begin": "955.6",
   "children": [],
   "end": "958.64",
   "id": "f000219",
   "language": "en",
   "lines": [
    "tab ramesh ne amit se kaha  jao"
   ]
  },
  {
   "begin": "958.64",
   "children": [],
   "end": "961.6",
   "id": "f000220",
   "language": "en",
   "lines": [
    "bhaabhee se bolo khaana laga den"
   ]
  },
  {
   "begin": "961.6",
   "children": [],
   "end": "962.68",
   "id": "f000221",
   "language": "en",
   "lines": [
    "jee achchha"
   ]
  },
  {
   "begin": "962.68",
   "children": [],
   "end": "966.48",
   "id": "f000222",
   "language": "en",
   "lines": [
    "aur amit uthakar baahar rasoee ghar kee taraph chala gaya"
   ]
  },
  {
   "begin": "966.48",
   "children": [],
   "end": "972.98",
   "id": "f000223",
   "language": "en",
   "lines": [
    "tab ramesh kumaar ne rikaard kee sueen neeche kar dee aur aavaaz kam kar dee"
   ]
  },
  {
   "begin": "972.98",
   "children": [],
   "end": "977.6",
   "id": "f000224",
   "language": "en",
   "lines": [
    "pramod ko bhee dev ne apane paas bitha liya tha"
   ]
  },
  {
   "begin": "977.6",
   "children": [],
   "end": "981",
   "id": "f000225",
   "language": "en",
   "lines": [
    "lekin pramod se baitha nahin gaya"
   ]
  },
  {
   "begin": "981",
   "children": [],
   "end": "984.04",
   "id": "f000226",
   "language": "en",
   "lines": [
    "vah dev kee god mein sir rakhakar let gaya"
   ]
  },
  {
   "begin": "984.04",
   "children": [],
   "end": "986.4",
   "id": "f000227",
   "language": "en",
   "lines": [
    "aaj phit ho gaya hai pramod"
   ]
  },
  {
   "begin": "986.4",
   "children": [],
   "end": "990.96",
   "id": "f000228",
   "language": "en",
   "lines": [
    "dev ne kaha aur ramesh kumaar kee or dekhane laga"
   ]
  },
  {
   "begin": "990.96",
   "children": [],
   "end": "992.6",
   "id": "f000229",
   "language": "en",
   "lines": [
    "aaj khush hai bahut"
   ]
  },
  {
   "begin": "992.6",
   "children": [],
   "end": "996.44",
   "id": "f000230",
   "language": "en",
   "lines": [
    "ramesh kumaar ne pramod kee or dekhate hue kaha"
   ]
  },
  {
   "begin": "996.44",
   "children": [],
   "end": "999.72",
   "id": "f000231",
   "language": "en",
   "lines": [
    "use lag raha tha pramod madahosh hai jaise"
   ]
  },
  {
   "begin": "999.72",
   "children": [],
   "end": "1009.92",
   "id": "f000232",
   "language": "en",
   "lines": [
    "lekin ramesh kee baat sunate hee usane apana sir uthaakar kaha  khush kyon na hooon mere yaar kee shaadee pakkee ho gaee hai"
   ]
  },
  {
   "begin": "1009.92",
   "children": [],
   "end": "1012.52",
   "id": "f000233",
   "language": "en",
   "lines": [
    "mere lie ye sabase achchhee khabar hai"
   ]
  },
  {
   "begin": "1012.52",
   "children": [],
   "end": "1016.72",
   "id": "f000234",
   "language": "en",
   "lines": [
    "phir kuchh ruk kar bola  ab to roj jashn chalega"
   ]
  },
  {
   "begin": "1016.72",
   "children": [],
   "end": "1018.08",
   "id": "f000235",
   "language": "en",
   "lines": [
    "kal main paartee doonga"
   ]
  },
  {
   "begin": "1018.08",
   "children": [],
   "end": "1021.86",
   "id": "f000236",
   "language": "en",
   "lines": [
    "aur phir pramod ne dev ke gaalon ko choom liya"
   ]
  },
  {
   "begin": "1021.86",
   "children": [],
   "end": "1025.26",
   "id": "f000237",
   "language": "en",
   "lines": [
    "vah bahut khush tha"
   ]
  }
 ]
}